{
  "name": "exafs",
  "version": "0.6.2",
  "description": "Tool for creation, validation and execution of ExaBGP messages",
  "main": "flowapp/static/ts/index.ts",
  "scripts": {
    "build": "npm run build-ts && npm run build-styles",
    "build-ts": "npx webpack",
    "build-styles": "npx sass flowapp/static/scss/style.scss flowapp/static/style.min.css --style compressed && npm run style-autoprefixer",
    "lint-styles": "npx stylelint 'flowapp/static/scss/**/*.scss'",
    "style-autoprefixer": "postcss flowapp/static/style.min.css --use=autoprefixer --output=flowapp/static/style.min.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CESNET/ExaFS.git"
  },
  "author": "Jiří Vraný, Petr Adamec, Josef Verich, Jakub Man",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CESNET/ExaFS/issues"
  },
  "homepage": "https://github.com/CESNET/ExaFS#readme",
  "dependencies": {
    "bootstrap": "^5.1.3"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.12",
    "autoprefixer": "^10.4.7",
    "postcss-cli": "^10.0.0",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
